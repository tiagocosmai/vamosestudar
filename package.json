{
  "name": "vamos-estudar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "WATCHPACK_POLLING=false NEXT_TELEMETRY_DISABLED=1 next dev",
    "dev:no-watch": "WATCHPACK_POLLING=false NEXT_TELEMETRY_DISABLED=1 next dev --turbo=false",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "build:clean": "rm -rf .next && NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules/.cache"
  },
  "dependencies": {
    "next": "15.2.4",
    "react": "^18",
    "react-dom": "^18",
    "lucide-react": "^0.294.0",
    "watchpack": "2.4.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
